{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "convert-csv-organisations": "csvtojson assets/csvFiles/organisations.csv > assets/jsonFiles/organisations.json",
    "convert-csv-options": "csvtojson assets/csvFiles/options_map.csv > assets/jsonFiles/options_map.json",
    "convert-csv-companies-options": "csvtojson assets/csvFiles/companies_options_map.csv > assets/jsonFiles/companies_options_map.json",
    "convert-csv": "npm run convert-csv-organisations && npm run convert-csv-options && npm run convert-csv-companies-options"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.0",
    "@expo-google-fonts/roboto": "^0.2.0",
    "@material-ui/core": "^4.12.3",
