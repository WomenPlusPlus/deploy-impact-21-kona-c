    "npm": "^8.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-browser": "^0.6.2",
    "react-native-datatable-component": "^2.0.0",
    "react-native-emoji": "^1.8.0",
